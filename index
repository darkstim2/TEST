  function processor(){
    return `
      <section class="panel">
        <h2>âš™ï¸ ĞŸÑ€Ğ¾Ñ†ĞµÑÑĞ¾Ñ€</h2>
        <p class="muted">Ğ¦ĞµĞ½Ñ‚Ñ€Ğ°Ğ»ÑŒĞ½Ñ‹Ğ¹ Ğ¸ÑÑ‚Ğ¾Ñ‡Ğ½Ğ¸Ğº Ğ¾Ñ‚ÑĞ»ĞµĞ¶Ğ¸Ğ²Ğ°Ğ½Ğ¸Ñ Ğ²Ğ°ÑˆĞ¸Ñ… Ñ€ĞµĞ·ÑƒĞ»ÑŒÑ‚Ğ°Ñ‚Ğ¾Ğ² Ğ² ĞºĞ¾Ğ¼Ğ¿Ğ»ĞµĞºÑĞµ.</p>
        
        <div class="cyber-line"></div>
        
        <div class="mt-12 grid-2">
          <div class="kpi">ĞšÑ€Ğ¸Ğ¾â€‘Ğ“ĞµĞ½Ğ¾Ğ¼ <strong>${S.cryo}</strong></div>
          <div class="kpi">ĞšĞ¾Ğ¼Ğ¿Ğ¾Ğ½ĞµĞ½Ñ‚Ñ‹ <strong>${S.components}</strong></div>
          <div class="kpi">Ğ§ĞµÑ€Ñ‚ĞµĞ¶Ğ¸ <strong>${S.blueprints}</strong></div>
          <div class="kpi">ĞœĞ¾Ğ´Ğ¸Ñ„Ğ¸ĞºĞ°Ñ†Ğ¸Ğ¹ <strong>${S.modsBuilt}</strong></div>
          <div class="kpi">Ğ£Ñ€Ğ¾Ğ²ĞµĞ½ÑŒ <strong>${S.level}</strong></div>
          <div class="kpi">Ğ˜Ğ½Ğ²ĞµĞ½Ñ‚Ğ°Ñ€ÑŒ <strong>${S.inventory.length}</strong></div>
        </div>
        
        <div class="cyber-line"></div>
        
        <div class="mt-16 grid-2">
