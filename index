  function renderGodLog(){
    if(!S.god) S.god = [];
    if(!S.god.length) return '<div class="note">Ğ”Ğ¸Ğ°Ğ»Ğ¾Ğ³ Ğ¿ÑƒÑÑ‚.</div>';
    return S.god.slice(-12).map(m => `
      <div class="panel" style="padding:10px;margin-bottom:8px;background:${m.me? 'rgba(55,166,255,.08)' : 'rgba(12,24,48,.55)'}">
        <div class="meta">${m.me?'>> Ğ’Ğ«':':: Ğ‘ĞĞ“'}</div>
        <div style="margin-top:4px">${m.text}</div>
      </div>`).join('');
  }

  // ---------- Handlers ----------
  function attach(name){
    // Global internal links
    $$("[data-go]").forEach(b=>b.addEventListener('click', e=>go(b.dataset.go)));

    if(name==='lab'){
      $('#extract').addEventListener('click', () => {
        const p = $('#pbar'); p.style.width='0%';
        let w = 0;
        const t = setInterval(()=>{
          w += Math.random()*22;
          p.style.width = Math.min(w, 100)+'%';
          if(w>=100){ clearInterval(t);
            const gain = 1 + Math.floor(Math.random()*3); // 1..3
            S.cryo += gain;
            S.log.push(`Ğ˜Ğ·Ğ²Ğ»ĞµÑ‡ĞµĞ½Ğ¸Ğµ Ğ·Ğ°Ğ²ĞµÑ€ÑˆĞµĞ½Ğ¾: +${gain} ĞšÑ€Ğ¸Ğ¾â€‘Ğ“ĞµĞ½Ğ¾Ğ¼`);
            toast(`Ğ“Ğ¾Ñ‚Ğ¾Ğ²Ğ¾: +${gain} â“€`);
            go('lab');
          }
        }, 220);
      });

      $('#extractComponents').addEventListener('click', () => {
        const n = 1 + Math.floor(Math.random()*4);
        S.components += n;
        S.log.push(`Ğ¡Ğ¸Ğ½Ñ‚ĞµĞ· ĞºĞ¾Ğ¼Ğ¿Ğ¾Ğ½ĞµĞ½Ñ‚Ğ¾Ğ²: +${n}`);
        toast(`+${n} ĞºĞ¾Ğ¼Ğ¿Ğ¾Ğ½ĞµĞ½Ñ‚Ğ¾Ğ²`);
        go('lab');
      });

      $('#makeBlueprint').addEventListener('click', () => {
        if(S.components < 2){ toast('ĞÑƒĞ¶Ğ½Ğ¾ 2 ĞºĞ¾Ğ¼Ğ¿Ğ¾Ğ½ĞµĞ½Ñ‚Ğ°'); return; }
        S.components -= 2; S.blueprints += 1;
        S.log.push('Ğ¡Ğ¾Ğ·Ğ´Ğ°Ğ½ Ñ‡ĞµÑ€Ñ‚Ñ‘Ğ¶: +1');
        toast('Ğ§ĞµÑ€Ñ‚Ñ‘Ğ¶ ÑĞ¾Ğ·Ğ´Ğ°Ğ½');
        go('lab');
      });
    }

    if(name==='mods'){
      $('#calibrate').addEventListener('click', ()=>{
        S.modules.calibrated = true; S.level = Math.max(S.level, 2);
        S.log.push('ĞšĞ°Ğ»Ğ¸Ğ±Ñ€Ğ¾Ğ²ĞºĞ° Ğ²Ñ‹Ğ¿Ğ¾Ğ»Ğ½ĞµĞ½Ğ°');
        toast('ĞœĞ¾Ğ´ÑƒĞ»ÑŒ Ğ¾Ñ‚ĞºĞ°Ğ»Ğ¸Ğ±Ñ€Ğ¾Ğ²Ğ°Ğ½');
        go('mods');
      });
      $('#freq').addEventListener('click', ()=>{
        const val = Math.floor(7 + Math.random()*93);
        S.modules.frequency = val;
        S.log.push(`Ğ§Ğ°ÑÑ‚Ğ¾Ñ‚Ğ° ÑƒÑÑ‚Ğ°Ğ½Ğ¾Ğ²Ğ»ĞµĞ½Ğ° Ğ½Ğ° ${val} Ğ“Ñ†`);
        toast(`Ğ§Ğ°ÑÑ‚Ğ¾Ñ‚Ğ°: ${val} Ğ“Ñ†`);
        go('mods');
      });
      $('#install').addEventListener('click', ()=>{
        if(!S.inventory.length){ toast('ĞĞµÑ‚ Ğ¼Ğ¾Ğ´ÑƒĞ»ĞµĞ¹ Ğ´Ğ»Ñ ÑƒÑÑ‚Ğ°Ğ½Ğ¾Ğ²ĞºĞ¸'); return; }
        const it = S.inventory.shift();
        S.modules.installed.push(it);
        S.modsBuilt += 1;
        S.log.push(`Ğ£ÑÑ‚Ğ°Ğ½Ğ¾Ğ²Ğ»ĞµĞ½ Ğ¼Ğ¾Ğ´ÑƒĞ»ÑŒ: ${it.name}`);
        toast(`Ğ£ÑÑ‚Ğ°Ğ½Ğ¾Ğ²Ğ»ĞµĞ½Ğ¾: ${it.name}`);
        go('mods');
      });
      $('#buildMod').addEventListener('click', ()=>{
        if(S.blueprints < 1 || S.components < 3){ toast('ĞÑƒĞ¶Ğ½Ğ¾ 1 Ñ‡ĞµÑ€Ñ‚Ñ‘Ğ¶ Ğ¸ 3 ĞºĞ¾Ğ¼Ğ¿Ğ¾Ğ½ĞµĞ½Ñ‚Ğ°'); return; }
        S.blueprints -= 1; S.components -= 3;
        const id = 'mod-'+Math.random().toString(16).slice(2,6);
        const name = 'ĞœĞ¾Ğ´â€‘'+id.slice(-3).toUpperCase();
        S.inventory.push({id, name});
        S.log.push(`ĞŸĞ¾Ğ»ÑƒÑ‡ĞµĞ½ Ğ¼Ğ¾Ğ´Ğ¸Ñ„Ğ¸ĞºĞ°Ñ‚Ğ¾Ñ€: ${name}`);
        toast(`ĞŸĞ¾Ğ»ÑƒÑ‡ĞµĞ½: ${name}`);
        go('mods');
      });
    }

    if(name==='shop'){
      $$('#view [data-buy]').forEach(btn => btn.addEventListener('click', () => {
        const id = btn.dataset.buy;
        const item = (window.__shop||[]).find(x => x.id===id);
        if(!item) return;
        if(S.cryo < item.price){ toast('ĞĞµĞ´Ğ¾ÑÑ‚Ğ°Ñ‚Ğ¾Ñ‡Ğ½Ğ¾ ĞšÑ€Ğ¸Ğ¾â€‘Ğ“ĞµĞ½Ğ¾Ğ¼Ğ°'); return; }
        S.cryo -= item.price;
        S.inventory.push({id:item.id, name:item.name});
        S.log.push(`ĞšÑƒĞ¿Ğ»ĞµĞ½Ğ¾: ${item.name} (-${item.price} â“€)`);
        toast('ĞšÑƒĞ¿Ğ»ĞµĞ½Ğ¾: '+item.name);
        go('shop');
      }));

      $('#doExchange').addEventListener('click', ()=>{
        const dir = $('#exDir').value;
        let n = Math.max(1, Math.floor(+$('#exAmount').value||1));
        if(dir==='c2g'){
          const need = 5*n;
          if(S.components < need){ toast(`ĞÑƒĞ¶Ğ½Ğ¾ ${need} ĞºĞ¾Ğ¼Ğ¿Ğ¾Ğ½ĞµĞ½Ñ‚Ğ¾Ğ²`); return; }
          S.components -= need; S.cryo += n;
          S.log.push(`ĞĞ±Ğ¼ĞµĞ½: -${need} ĞºĞ¾Ğ¼Ğ¿. +${n} â“€`);
        }else{
          if(S.cryo < n){ toast(`ĞÑƒĞ¶Ğ½Ğ¾ ${n} â“€`); return; }
          S.cryo -= n; const comp = 4*n; S.components += comp;
          S.log.push(`ĞĞ±Ğ¼ĞµĞ½: -${n} â“€ +${comp} ĞºĞ¾Ğ¼Ğ¿.`);
        }
        toast('ĞĞ±Ğ¼ĞµĞ½ Ğ²Ñ‹Ğ¿Ğ¾Ğ»Ğ½ĞµĞ½');
        go('shop');
      });
    }

    if(name==='god'){
      $('#sendGod').addEventListener('click', sendGod);
      $('#seed').addEventListener('click', ()=>{
        S.god = (S.god||[]);
        S.god.push({me:true, text:'/start'});
        S.god.push({me:false, text:'NeoLab [BLUE] ÑĞ»ÑƒÑˆĞ°ĞµÑ‚. Ğ”Ğ¾ÑÑ‚ÑƒĞ¿Ğ½Ñ‹ /help Ğ¸ Ğ±Ğ°Ğ·Ğ¾Ğ²Ñ‹Ğµ Ğ·Ğ°Ğ¿Ñ€Ğ¾ÑÑ‹.'});
        S.god.push({me:true, text:'ĞšĞ°Ğº Ğ¿Ğ¾Ğ²Ñ‹ÑĞ¸Ñ‚ÑŒ ÑƒÑ€Ğ¾Ğ²ĞµĞ½ÑŒ?'});
        S.god.push({me:false, text:'Ğ˜Ğ·Ğ²Ğ»ĞµĞºĞ°Ğ¹Ñ‚Ğµ ĞšÑ€Ğ¸Ğ¾â€‘Ğ“ĞµĞ½Ğ¾Ğ¼, ÑĞ¾Ğ·Ğ´Ğ°Ğ²Ğ°Ğ¹Ñ‚Ğµ Ñ‡ĞµÑ€Ñ‚ĞµĞ¶Ğ¸ Ğ¸ ÑƒÑÑ‚Ğ°Ğ½Ğ°Ğ²Ğ»Ğ¸Ğ²Ğ°Ğ¹Ñ‚Ğµ Ğ¼Ğ¾Ğ´ÑƒĞ»Ğ¸ â€” Ğ¾Ğ¿Ñ‹Ñ‚ Ğ¿Ñ€Ğ¸Ğ´Ñ‘Ñ‚.'});
        go('god');
      });
    }
  }

  function sendGod(){
    const input = $('#godInput');
    const text = (input.value||'').trim();
    if(!text) return;
    S.god = (S.god||[]); S.god.push({me:true, text});
    let reply = 'â€¦';
    if(text.startsWith('/start')) reply = 'Ğ“Ğ¾Ñ‚Ğ¾Ğ² Ğº Ñ€Ğ°Ğ±Ğ¾Ñ‚Ğµ. ĞĞ°Ğ¶Ğ¼Ğ¸Ñ‚Ğµ ĞºĞ½Ğ¾Ğ¿ĞºĞ¸ Ğ²Ğ½Ğ¸Ğ·Ñƒ Ğ´Ğ»Ñ Ğ½Ğ°Ğ²Ğ¸Ğ³Ğ°Ñ†Ğ¸Ğ¸.';
    else if(text.startsWith('/help')) reply = 'ĞšĞ¾Ğ¼Ğ°Ğ½Ğ´Ñ‹: /start, /help. Ğ”ĞµĞ¹ÑÑ‚Ğ²Ğ¸Ñ: Ğ¸Ğ·Ğ²Ğ»ĞµÑ‡ĞµĞ½Ğ¸Ğµ, ĞºĞ°Ğ»Ğ¸Ğ±Ñ€Ğ¾Ğ²ĞºĞ°, Ğ¿Ğ¾ĞºÑƒĞ¿ĞºĞ° Ğ¼Ğ¾Ğ´Ğ¾Ğ².';
    else reply = 'Ğ¯ ÑĞ»Ñ‹ÑˆÑƒ Ñ‚ĞµĞ±Ñ. Ğ›Ğ°Ğ±Ğ¾Ñ€Ğ°Ñ‚Ğ¾Ñ€Ğ¸Ñ Ğ¿Ğ¾Ğ¼Ğ¾Ğ¶ĞµÑ‚ Ğ´Ğ¾Ğ±Ñ‹Ñ‚ÑŒ â“€, Ğ¼Ğ°Ğ³Ğ°Ğ·Ğ¸Ğ½ â€” ĞºĞ¾Ğ½Ğ²ĞµÑ€Ñ‚Ğ¸Ñ€Ğ¾Ğ²Ğ°Ñ‚ÑŒ Ñ€ĞµÑÑƒÑ€ÑÑ‹.';
    S.god.push({me:false, text:reply});
    input.value = '';
    go('god');
  }

